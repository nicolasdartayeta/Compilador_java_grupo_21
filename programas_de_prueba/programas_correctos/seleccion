programa_if

begin
ulongint a;
/# Variable con tipo embebido, no hace falta declararla #/
x := 1;
if (x < 5) then
   a := a+1;
else
    a:=a-1;
end_if;

typedef Struct <ulongint, single> [
    d,
    e
] myStructB;

myStructB r;

if (a=1) then
    if (x != 10) then
        r.e := 5.0;
    else
        a := 20;
    end_if;
end_if;

x:=30;

if (x < 50) then
    outf(x+5);
end_if;
end