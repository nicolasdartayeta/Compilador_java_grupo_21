programa_for

begin
    ulongint j;
    /# Variable X con tipo embebido, no hace falta declararla #/
    for(x := 10; x >= 0; down 1)
    begin
        for (j:=5; j > 0; down 2)
        begin
            x := x + 1;
        end;
    end;
    single c;
    for(x := 10; x > 0; up 2; (x < 100))
    begin
        x := x - 6 / 3;
    end;

    outf( {Programa terminado} );
end

